    sleep 2\
    sudo reboot\
else\
    echo -e "${RED}-----------------------------------------${RESET}"\
    echo -e "${RED}       Installation Script Ended${RESET}"\
    echo -e "${RED}-----------------------------------------${RESET}"\
    exit 0\
fi\
