: 1762591107:0;#!/usr/bin/env bash\
\
# --- CONFIG ---\
WALLPAPER_DIR="$HOME/Pictures/Wallpapers"\
CACHE_DIR="$HOME/.cache/wall-thumbs"\
THUMB_SIZE="240x135"\
ICON_SIZE="110px"\
TRANSITION="center"\
DURATION="1"\
SHOW_NAMES=false\
# ---------------\
\
mkdir -p "$CACHE_DIR"\
\
# generate thumbnails if not cached\
for img in "$WALLPAPER_DIR"/*.jpg; do\
    thumb="$CACHE_DIR/$(basename "$img")"\
    [[ -f "$thumb" ]] || magick convert "$img" -resize "$THUMB_SIZE^" -gravity center -extent "$THUMB_SIZE" "$thumb"\
done\
\
# build rofi entries\
entries=""\
for img in "$WALLPAPER_DIR"/*.jpg; do\
    name="$(basename "$img")"\
    thumb="$CACHE_DIR/$name"\
    if [[ "$SHOW_NAMES" == true ]]; then\
        entries+="$name\x00icon\x1f$thumb\n"\
    else\
        entries+="\x00icon\x1f$thumb\n"\
    fi\
done\
\
# launch rofi\
chosen=$(echo -en "$entries" | rofi -dmenu -show-icons -theme-str "\
window {\
    width: 70%;\
    height: 65%;\
    location: center;\
}\
listview {\
    columns: 6;\
    spacing: 15px;\
    dynamic: true;\
    layout: horizontal;\
}\
element {\
    padding: 6px;\
    orientation: vertical;\
    spacing: 3px;\
    align: 0.5;\
}\
element-icon {\
    size: $ICON_SIZE;\
    border-radius: 10px;\
}\
entry {\
    placeholder: \"Search wallpaper...\";\
}\
" -p "Select wallpaper:")\
\
# --- APPLY WALLPAPER ---\
if [[ -n "$chosen" ]]; then\
    # map selection back to original image\
    if [[ "$SHOW_NAMES" == true ]]; then\
        selected="$WALLPAPER_DIR/$chosen"\
    else\
        # fallback: extract thumbnail filename\
        filename=$(basename "$chosen")\
        selected="$WALLPAPER_DIR/$filename"\
    fi\
\
    # swww\
    if command -v swww &>/dev/null; then\
        swww img "$selected" --transition-type "$TRANSITION" --transition-duration "$DURATION"\
    fi\
\
    # pywal\
    if command -v wal &>/dev/null; then\
        wal -i "$selected"\
    fi\
\
    # refresh Waybar\
    pkill -SIGUSR1 waybar\
\
    notify-send "Wallpaper switched "$(basename "$selected")"\
fi\
\
\
\
\
\

