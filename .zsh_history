: 1762523746:0;#!/bin/bash\
\
# Directory containing wallpapers\
WALL_DIR="$HOME/Pictures/Wallpapers"\
\
# Current directory (to return to)\
CWD="$(pwd)"\
\
# Change to wallpaper directory\
cd "$WALL_DIR" || exit\
\
# Handle spaces in filenames properly\
IFS=$'\n'\
\
# Use rofi in dmenu/grid mode with icon support to select wallpaper\
SELECTED_WALL=$(for img in *.jpg *.png; do\
    # Output in format: filename<unit separator>filename newline (rofi icon support)\
    echo -en "$img\x1f$img\n"\
done | rofi -dmenu -i -format 'i' -p "Select Wallpaper" -theme-str 'window { width: 700px; height: 400px; } listview { lines: 3; columns: 5; }')\
\
# If a wallpaper was selected\
if [[ -n "$SELECTED_WALL" ]]; then\
    # Apply wallpaper with swww using transition\
    swww img "$SELECTED_WALL" --transition-type="center" --transition-step=1 --transition-fps="60"\
\
    # Generate pywal colors\
    wal -i "$SELECTED_WALL" -n -s -t -e\
\
    # Reload waybar\
    pkill waybar\
    waybar > /dev/null 2>&1 &\
fi\
\
# Return to original directory\
cd "$CWD" || exit\

